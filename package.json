{
    "name": "@dhis2/app-service-data",
    "version": "0.1.0",
    "main": "build/cjs/index.js",
    "module": "build/es/index.js",
    "types": "build/types/index.d.ts",
    "repository": "https://github.com/amcgee/dhis2-app-platform",
    "author": "Austin McGee <austin@dhis2.org>",
    "license": "BSD-3-Clause",
    "private": true,
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.4.0",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
        "@babel/plugin-transform-runtime": "^7.4.0",
        "@babel/preset-env": "^7.4.1",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.3.3",
        "@babel/runtime": "^7.4.0",
        "@dhis2/cli-helpers-engine": "^0.11.0",
        "@dhis2/cli-style": "^2.2.2",
        "@types/jest": "^24.0.11",
        "concurrently": "^4.1.0",
        "husky": "^1.3.1",
        "jest": "^24.5.0",
        "rimraf": "^2.6.3",
        "ts-jest": "^24.0.0",
        "typescript": "^3.3.4000"
    },
    "peerDependencies": {
        "react": "^16.8",
        "react-dom": "^16.8"
    },
    "dependencies": {
        "@types/react": "^16.8.8",
        "@types/react-dom": "^16.8.2"
    },
    "scripts": {
        "clean": "rimraf ./build/*",
        "build:commonjs": "BABEL_ENV=commonjs babel src/** --out-dir ./build/cjs --extensions \".ts,.tsx\" --source-maps inline --verbose",
        "build:modules": "BABEL_ENV=modules babel src/** --out-dir ./build/es --extensions \".ts,.tsx\" --source-maps inline --verbose",
        "build:types": "tsc --emitDeclarationOnly --outDir ./build/types",
        "build": "NODE_ENV=production yarn clean && concurrently -n cjs,es,types \"yarn build:commonjs\" \"yarn build:modules\" \"yarn build:types\"",
        "watch": "NODE_ENV=development yarn clean && concurrently -n cjs,es,types \"yarn build:commonjs --watch\" \"yarn build:modules --watch\" \"yarn build:types --watch\"",
        "type-check": "tsc --noEmit --allowJs --checkJs",
        "type-check:watch": "yarn type-check --watch",
        "test": "jest src/*",
        "prepublishOnly": "yarn build",
        "format": "d2-style js apply --all --no-stage"
    },
    "husky": {
        "hooks": {
            "commit-msg": "d2-style commit check",
            "pre-commit": "d2-style js apply"
        }
    }
}
